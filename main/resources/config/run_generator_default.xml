<?xml version="1.0" encoding="utf-8"?>
<GeneratorConfiguration>
	<rng>
		<engine>trng/mrg2</engine>
		<seed>1</seed>
	</rng>
	<household_profile>./data/households_flanders.xml</household_profile>
	<population_size>100000</population_size>
	<geoprofile>
		<cities>./data/belgium_population.csv</cities>
		<villages>./data/geo_villages.csv</villages>
		<use_xy>false</use_xy>
		<in_villages>0.5</in_villages>
		<scale>1.05</scale>
	</geoprofile>
	<school>
		<size>500</size>
		<age>
			<minimum>3</minimum>
			<maximum>17</maximum>
		</age>
	</school>
	<university>
		<size>3000</size>
		<fraction>0.25</fraction>
		<age>
			<minimum>18</minimum>
			<maximum>23</maximum>
		</age>
	</university>
	<work>
		<size>500</size>
		<fraction>0.70</fraction>
		<age>
			<minimum>18</minimum>
			<maximum>65</maximum>
		</age>
	</work>
	<community>
		<size>2000</size>
		<add_complete_household>true</add_complete_household>
	</community>
</GeneratorConfiguration>
